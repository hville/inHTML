function inHTML(e){const t=document.currentScript,n=t.previousElementSibling,i={};for(let e of n.getElementsByTagName("template")){for(let t of e.content.children)t.id&&(i[t.id]=t).removeAttribute("id");e.remove()}return t.remove(),e(n,i)}!function(){const e=document.createTreeWalker(document,1),t={};inHTML.data={},inHTML.refs={},inHTML.clone=function(t){const n=t.cloneNode(!0);let i;for(n.refs={},e.currentNode=n;i=e.nextNode();)i.id&&(n.refs[i.id]=i).removeAttribute("id");return n},inHTML.delegate=function(e){if(!t[e]){const n="on"+e[0].toUpperCase()+e.slice(1);document.addEventListener(e,(function(e){let t=e.target;do{if(t[n])return t[n](e)}while(t=t.parentNode)})),t[e]=!0}},inHTML.list=function(e,t,n,i=null,r=null){return{parent:e,getKey:t,factory:n,after:i,before:r,map:Object.create(null),update:function(e){const t=this.parent,n=Object.create(null);let i=this.after?this.after.nextSibling:t.firstChild;if(e.length||this.before||this.after){for(let r=0;r<e.length;++r){const o=this.getKey(e[r],r,e);let l=this.map[o];l?l.update&&l.update(e[r],o,e):l=this.factory(e[r],r,e),n[o]=l,i?l===i.nextSibling?t.removeChild(i):l!==i&&t.insertBefore(l,i):t.appendChild(l),i=l.nextSibling}for(;i!==this.before;){const e=i.nextSibling;t.removeChild(i),i=e}}else t.textContent="";return this.map=n,this}}}}();